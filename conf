./configure --sysconfdir=/etc/zabbix --enable-server --with-libcurl --with-libxml2 --with-ssh2 --with-net-snmp --with-openipmi --with-jabber --with-openssl --with-unixodbc --with-ldap --with-oracle-lib=/usr/lib/oracle/19.6/client64/lib --with-oracle-include=/usr/include/oracle/19.6/client64

./configure --prefix=/zabbix_server --enable-server --with-oracle --with-net-snmp --with-libcurl --with-libxml2

